{
  "swagger": "2.0",
  "info": {
    "version": "0.0.1",
    "title": "ART Search"
  },
  "host": "localhost:10010",
  "basePath": "/",
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/art": {
      "x-swagger-router-controller": "art",
      "get": {
        "description": "Returns links to some art",
        "operationId": "getArt",
        "parameters": [
          {
            "name": "searchTerm",
            "in": "query",
            "description": "The type of art to search",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/GetArtResponse"
            }
          },
          "default": {
            "description": "Error",
            "schema": {
              "$ref": "#/definitions/ErrorResponse"
            }
          }
        }
      }
    },
    "/swagger": {
      "x-swagger-pipe": "swagger_raw"
    }
  },
  "definitions": {
    "GetArtResponse": {
      "required": [
        "art"
      ],
      "properties": {
        "art": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "id"
            ],
            "properties": {
              "id": {
                "type": "string"
              },
              "url": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "ErrorResponse": {
      "required": [
        "message"
      ],
      "properties": {
        "message": {
          "type": "string"
        }
      }
    }
  }
}